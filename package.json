{
  "name": "vite-react-tailwind-template",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:ci": "tsc && vite build",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test serve:dev http://localhost:3000/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test serve http://localhost:3000/ 'cypress run'",
    "serve": "npm run build && vite preview --port 3000",
    "serve:dev": "vite --port 3000",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
